generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model cats {
  id          String?   @id @default(auto()) @map("_id") @db.ObjectId
  description String?
  eventName   String?
  eventDate   DateTime @db.Date
  city        String?
  venue       String?
  seatNumber  String?
  price       Float
  isSold      Boolean  @default(false)
}
